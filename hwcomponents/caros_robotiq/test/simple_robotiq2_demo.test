<launch>
  <group>
    <!-- Loading default pameters for hand ip and tcp port. -->
    <include file="$(find caros_robotiq)/launch/caros_robotiq2_param.xml" ns="robotiq2_simple_demo_node">
      <!-- no parameters have to be set - relying on default values -->
    </include>
    <node pkg="caros_robotiq" type="caros_robotiq2" name="robotiq2_simple_demo_node" required="true" output="screen">
      <!-- Nothing -->
    </node>
  </group>

  <group>
    <node pkg="caros_robotiq" type="simple_caros_robotiq2_demo" name="caros_robotiq2_simple_demo" required="true" output="screen">
    </node>
  </group>
</launch>
