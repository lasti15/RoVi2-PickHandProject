<launch>
  <group>
    <!-- Loading default pameters for hand ip and tcp port. -->
    <include file="$(find caros_robotiq)/launch/caros_robotiq3_param.xml" ns="caros_robotiq3">
      <!-- no parameters have to be set - relying on default values -->
    </include>
    <node pkg="caros_robotiq" type="caros_robotiq3" name="caros_robotiq3" required="true" output="screen">
      <!-- Nothing -->
    </node>
  </group>

  <group>
    <node pkg="caros_robotiq" type="simple_caros_robotiq3_demo" name="caros_robotiq3_simple_demo" required="true" output="screen">
    </node>
  </group>
</launch>
