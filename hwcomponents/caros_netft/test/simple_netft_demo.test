<launch>
  <group>
    <!-- Loading default parameters for hand ip and port. -->
    <include file="$(find caros_netft)/launch/caros_netft_param.xml" ns="caros_netft">
      <!-- no parameters have to be set - relying on default values -->
    </include>
    <!-- The 'name' parameter has to match the 'ns' parameter used in the include tag above -->
    <node pkg="caros_netft" type="caros_netft_node" name="caros_netft" required="true" output="screen">
      <!-- Nothing -->
    </node>
  </group>

  <group>
    <node pkg="caros_netft" type="simple_caros_netft_demo" name="simple_caros_netft_demo" required="true" output="screen">
    </node>
  </group>
</launch>
