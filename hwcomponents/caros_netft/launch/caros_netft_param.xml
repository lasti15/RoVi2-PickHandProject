<!-- Parameters for the caros_netft node -->
<launch>
  <!-- Define the parameterisation of the ROS parameters -->
  <arg name="ip" default="192.168.100.2" />
  <arg name="port" default="49152" />
  <arg name="rate" default="40" />

  <!-- Set the ROS parameters -->
  <param name="ip" type="str" value="$(arg ip)" />
  <param name="port" type="str" value="$(arg port)"/>
  <param name="rate" type="str" value="$(arg rate)"/>
</launch>
